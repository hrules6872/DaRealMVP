<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hrules.darealmvp.sample">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".presentation.views.MainActivityView">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name=".presentation.views.ListActivityView"/>
    <activity android:name=".presentation.views.PreferenceActivityView"/>
    <activity android:name=".presentation.views.PagerActivityView"/>
  </application>

</manifest>
